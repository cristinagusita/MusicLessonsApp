<?import javafx.scene.text.*?>

<AnchorPane prefHeight="680.0" prefWidth="675.0" style="-fx-background-color: #f4e7d5;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.java11project.sample.Controllers.Client.ClientReviewController">
    <children>
        <ImageView fitHeight="150.0" fitWidth="365.0" layoutX="170.0" layoutY="530.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@com/example/java11project/rev.jpg" />
            </image>
        </ImageView>
        <ComboBox fx:id="cb1" layoutX="223.0" layoutY="200.0" prefHeight="38.0" prefWidth="218.0" style="-fx-background-color: #e7c1ab;" />
        <Button fx:id="search" layoutX="222.0" layoutY="99.0" mnemonicParsing="false" onAction="#buttonOneOnAction" style="-fx-background-color: #e7c1ab;" text="Press to search profesors">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Button>
        <Text layoutX="170.0" layoutY="176.0" strokeType="OUTSIDE" strokeWidth="0.0" text="You can choose someone from the list below:">
            <font>
                <Font name="System Bold" size="17.0" />
            </font>
        </Text>
        <Button fx:id="review" layoutX="253.0" layoutY="424.0" mnemonicParsing="false" onAction="#buttonTwoOnAction" style="-fx-background-color: #e7c1ab;" text="REVIEW HIM/HER">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Button>
        <Text fx:id="choose" layoutX="246.0" layoutY="295.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Now choose a grade: ">
            <font>
                <Font name="System Bold" size="17.0" />
            </font>
        </Text>
        <ComboBox fx:id="cb2" layoutX="223.0" layoutY="340.0" prefHeight="38.0" prefWidth="218.0" style="-fx-background-color: #e7c1ab;" />
    </children>
</AnchorPane>
